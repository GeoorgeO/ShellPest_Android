<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. MaterialComponents.DayNight.DarkActionBar-->
    <style name="Theme.Oscuro" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/verde_azul</item>
        <item name="colorPrimaryVariant">@color/verde_azul</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/verde_azul</item>
        <item name="colorSecondaryVariant">@color/verde_azul</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:textColor">@color/black</item>

    </style>
</resources>